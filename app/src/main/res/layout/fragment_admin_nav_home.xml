<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".Admin.ui.admin_nav_home">

    <com.google.android.material.appbar.AppBarLayout
        android:layout_width = "match_parent"
        android:layout_height = "wrap_content">
        <androidx.appcompat.widget.Toolbar
            android:background = "@color/colorPrimary"
            android:layout_width = "match_parent"
            app:title="HOME"
            android:layout_height = "?attr/actionBarSize"
            app:layout_scrollFlags = "scroll|snap|enterAlways"
            app:theme = "@style/ThemeOverlay.AppCompat.Dark.ActionBar"
            app:popupTheme = "@style/ThemeOverlay.AppCompat.Light" />
    </com.google.android.material.appbar.AppBarLayout>


    <!-- TODO: Update blank fragment layout -->
    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="50dp">

        <ImageView
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:background="@drawable/skincare"
            android:layout_margin="25dp"
            android:id="@+id/img1"
            />
        <ImageView
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:background="@drawable/mendress"
            android:id="@+id/img2"
            android:layout_toRightOf="@id/img1"
            android:layout_margin="25dp"
            />
        <ImageView
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:background="@drawable/dressc"
            android:layout_toRightOf="@id/img2"
            android:layout_margin="25dp"
            android:id="@+id/img3"
            />
        <ImageView
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:background="@drawable/ic_furniture"
            android:layout_margin="25dp"
            android:id="@+id/img4"
            android:layout_below="@id/img1"
            />
        <ImageView
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:background="@drawable/ic_books"
            android:layout_margin="25dp"
            android:id="@+id/img5"
            android:layout_below="@id/img2"
            android:layout_toRightOf="@id/img4"
            />
        <ImageView
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:background="@drawable/ic_toys"
            android:layout_margin="25dp"
            android:id="@+id/img6"
            android:layout_below="@id/img3"
            android:layout_toRightOf="@id/img5"
            />
        <ImageView
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:background="@drawable/ic_television"
            android:layout_margin="25dp"
            android:id="@+id/img7"
            android:layout_below="@id/img4"
            />
        <ImageView
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:background="@drawable/ic_gym"
            android:layout_margin="25dp"
            android:id="@+id/img8"
            android:layout_below="@id/img4"
            android:layout_toRightOf="@id/img7"
            />
        <ImageView
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:background="@drawable/ic_mobile"
            android:layout_margin="25dp"
            android:id="@+id/img9"
            android:layout_below="@id/img5"
            android:layout_toRightOf="@id/img8"
            />
        <ImageView
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:background="@drawable/kitchenutensils"
            android:layout_margin="25dp"
            android:id="@+id/img10"
            android:layout_below="@id/img7"

            />
        <ImageView
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:background="@drawable/beverages"
            android:layout_margin="25dp"
            android:id="@+id/img11"
            android:layout_below="@id/img8"
            android:layout_toRightOf="@id/img10"
            />
        <ImageView
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:background="@drawable/prescription"
            android:layout_margin="25dp"
            android:id="@+id/img12"
            android:layout_below="@id/img8"
            android:layout_toRightOf="@id/img8"
            />
        <Button
            android:layout_width="300dp"
            android:layout_height="wrap_content"
            android:id="@+id/item_newcatogry"
            android:layout_below="@id/img12"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="12dp"
            android:background="#c1eb"
            android:text="New Catogry"
            />


    </RelativeLayout>
    </androidx.core.widget.NestedScrollView>


</FrameLayout>